{
    "name": "jquery-comments",
    "version": "0.1.0",
    "author": "Joona Tykkyl√§inen",
    "description": "A starter project for making jQuery plugins.",
    "license": "MIT",
    "keywords": [
        "jquery",
        "javascript",
        "plugin",
        "commenting"
    ],
    "devDependencies": {
        "jquery": "^1.9.0",
        "jshint": "^2.5.5",
        "normalize.css": "^3.0.1",
        "uglify-js": "^2.4.15",
        "jasmine": "2.1.1",
        "karma": "0.12.28",
        "karma-cli": "0.0.4",
        "karma-jasmine": "0.3.2",
        "karma-jasmine-jquery": "0.1.1",
        "karma-chrome-launcher": "0.1.7",
        "karma-firefox-launcher": "0.1.3",
        "sinon": "1.12.1",
        "karma-sinon": "1.0.4",
        "karma-junit-reporter": "0.2.2"
    },
    "scripts": {
        "prestart": "npm install && ./node_modules/uglify-js/bin/uglifyjs js/jquery-comments.js -o target/jquery-comments.min.js",
        "pretest": "npm install && ./node_modules/.bin/jshint js/",
        "test": "karma start test/karma.conf.js",
        "test-single-run": "karma start testd/karma.conf.js --single-run",
        "clean": "rm -rf node_modules && npm install"
    },
    "jshintConfig": {
        "browser": true,
        "node": true,
        "unused": true,
        "undef": true,
        "globals": {
            "$": false,
            "jQuery": false
        }
    },
    "preferGlobal": true
}
